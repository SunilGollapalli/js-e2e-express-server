pipeline{
    agent any
    stages{
        stage('Git download') {
            steps{
                git branch : 'main', url: 'https://github.com/SunilGollapalli/js-e2e-express-server.git'
            }
        }
        stage('build'){
            steps{
                sh 'npm install'
                sh 'npm build'
                sh 'npm pack'

            }

        }
        post{
            success{
                junit '**/TEST-.xml'
            }
        }
    }
}